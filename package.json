{
  "name": "slugify",
  "version": "0.0.1",
  "description": "Slugify | Online Slug Generator",
  "main": "webpack.config.js",
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "css-loader": "^0.23.0",
    "es6-promise": "^3.0.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "jest-cli": "^0.8.0",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "scripts": {
    "build": "webpack -p --progress --colors",
    "watch": "webpack --watch --colors",
    "test": "jest",
    "dev": "webpack-dev-server -d --progress --colors --hot --inline --host 0.0.0.0",
    "backend": "python src/app.py"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mozillazg/slugify.git"
  },
  "author": "mozillazg <mozillazg101@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mozillazg/slugify/issues"
  },
  "homepage": "https://github.com/mozillazg/slugify"
}
